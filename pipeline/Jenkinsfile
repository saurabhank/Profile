pipeline {
    agent any 
    tools {nodejs "node"}

    stages {
        stage('Example') {
            steps {

                sh 'npm config ls'
            }
        }   
         stage('build') {
            steps {

                sh 'npm i'
            }
        }    
        stage('start server'){
            steps {
                sh 'chmod 777 run.sh'
                sh 'BUILD_ID=dontKillMe ./run.sh'
            }
        } 
    }
}
